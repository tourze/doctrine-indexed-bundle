{
  "name": "tourze/doctrine-indexed-bundle",
  "description": "Doctrine索引增强",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/doctrine-bridge": "^6.4",
    "symfony/http-kernel": "^6.4",
    "yiisoft/strings": "^2.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineIndexedBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineIndexedBundle\\Tests\\": "tests"
    }
  }
}