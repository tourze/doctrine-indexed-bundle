{
  "name": "tourze/doctrine-indexed-bundle",
  "description": "A Symfony bundle that enhances Doctrine ORM with automatic index management using PHP 8 attributes",
  "license": "MIT",
  "require": {
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/http-kernel": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-dependency-service-loader": "0.0.*",
    "yiisoft/strings": "^2.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/framework-bundle": "^7.3",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "0.0.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineIndexedBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineIndexedBundle\\Tests\\": "tests"
    }
  }
}